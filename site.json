{
  "baseUrl": "/2019",
  "titlePrefix": "TE3201 Website",
  "ignore": [
    "_site/*",
    ".idea/*",
    ".gitignore",
    "README.md",
    "schedule/admin/*",
    "*.md",
    "diagrams/*",
    "book/diagrams/*",
    ".git/*"
  ],
  "pages": [
    {"src": "index.md",
      "searchable": "no"},
    {"src": "admin/te3201-admin.md"},
    {"glob": "schedule/index.md",
      "searchable": "no"},
    {"glob": "schedule/summary.md",
      "searchable": "no"},
    {"glob": "schedule/**/index.md",
      "searchable": "no"},
    {"glob": "book/about/acknowledgements.md"},
    {"glob": "se-book-adapted/index.md"},
    {"glob": "se-book-adapted/chapters/*.md"},
    {"glob": "se-book-adapted/chapters-printable/*.md",
      "searchable": "no"
    },
    {"glob": "se-book-adapted/common/*.md"},
    {"glob": "book/combined/exercises.md"},
    {"src": "se-book-adapted/print.md",
      "searchable": "no"
    },
    {"glob": "se-book-adapted/chapters/architecture.md",
      "searchable": "no"
    },
    {"glob": "se-book-adapted/chapters/cppToJava.md",
      "searchable": "no"
    },
    {"glob": "se-book-adapted/chapters/designApproaches.md",
      "searchable": "no"
    },
    {"glob": "se-book-adapted/chapters/designPatterns.md",
      "searchable": "no"
    },
    {"glob": "se-book-adapted/chapters/gitAndGithub.md",
      "searchable": "no"
    },
    {"glob": "se-book-adapted/chapters/javaTools.md",
      "searchable": "no"
    },
    {"glob": "se-book-adapted/chapters/junit.md",
      "searchable": "no"
    },
    {"glob": "se-book-adapted/chapters/principles.md",
      "searchable": "no"
    },
    {"glob": "se-book-adapted/chapters/intellij.md",
      "searchable": "no"
    },
    {"glob": "book/**/index.md",
      "searchable": "no"
    },
    {"glob": "programming/index.md",
      "searchable": "yes"
    },
    {"glob": "programming/toc/*.md",
      "searchable": "yes"
    }
  ],
  "enableSearch": true,
  "headingIndexingLevel": 5
}
